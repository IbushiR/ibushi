<h1>ibutter</h1>

<%= @user.name %>

<%= link_to "プロフィール", profile_path %>

<%= link_to "ツイート", new_tweet_path %>


<%= button_to "ログアウト", destroy_user_session_path, method: :delete %>

<p>自分のツイート</p>

<% @user.tweets.each do |tweet| %>
  <ul>
    <li><%= tweet.content %></li>
  </ul>
<% end %>

<br>

<p>みんなの反応</p>

<% @users.each do |user| %>
  <% user.tweets.each do |tweetes| %>
    <ul>
      <li>
        <h5><%= user.name %><br></h5>
        <%= tweetes.content %><br>
        <%= tweetes.created_at.strftime('%Y/%m/%d %H:%M') %>
      </li>
    </ul>
  <% end %>
<% end %>


